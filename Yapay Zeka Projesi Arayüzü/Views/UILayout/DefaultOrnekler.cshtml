        <!-- Portfolio item 1 modal popup-->
        @foreach (var nesne in Model)
        {
            <div class="portfolio-modal modal fade" id="portfolioModal@(nesne.Ornekid)" tabindex="-1" role="dialog" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="close-modal" data-bs-dismiss="modal"><img src="~/Templates/agency/assets/img/Close.png" alt="Close modal" /></div>
                        <div class="container">
                            <div class="row justify-content-center">
                                <div class="col-lg-8">
                                    <div class="modal-body">
                                        <!-- Project details-->
                                        <h2 class="text-uppercase">@nesne.Ad.ToUpper()</h2>
                                        <img class="img-fluid d-block mx-auto" src="@nesne.ResimUrl" alt="..." />
                                        <table class="table table-bordered table-hover">
                                            <tr>
                                                <th>Ürün Adı</th>
                                                <th>İçindekiler</th>
                                                <th>Miktar</th>
                                                <th>Protein(gr)</th>
                                                <th>Karbonhidrat(gr)</th>
                                                <th>Kalori</th>

                                            </tr>

                                            
                                            
                                                <tr>
                                                    <td>@nesne.Ad</td>
                                                    <td>@nesne.ModelIcindekiler</td>
                                                    <td>@(nesne.ModelMiktar + "gr")</td>
                                                    <td>@(nesne.ModelProtein + "gr")</td>
                                                    <td>@(nesne.ModelKarbonhidrat + "gr")</td>
                                                    <td>@nesne.ModelKalori</td>
                                                </tr>
                                            

                                        </table>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }

