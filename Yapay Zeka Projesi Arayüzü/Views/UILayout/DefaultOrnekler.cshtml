@model List<Yapay_Zeka_Projesi_Arayüzü.Models.Analizler>


<section class="page-section bg-light" id="portfolio">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">ÖRNEK ANALİZLER</h2>
            <h3 class="section-subheading text-muted">Görseller ve Analizler Temsilidir.</h3>
        </div>
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-4 col-sm-6 mb-4">
                    <!-- Portfolio item 1-->
                    <div class="portfolio-item">
                        <a class="portfolio-link" data-bs-toggle="modal" href="#portfolioModal@(item.Ornekid)">
                            <div class="portfolio-hover">
                                <div class="portfolio-hover-content"><i class="fas fa-plus fa-3x"></i></div>
                            </div>
                            <img class="img-fluid" style="display:block; margin-left:auto;margin-right:auto;width:200px;height:150px" src="@item.ResimUrl" alt="..." />
                        </a>
                        <div class="portfolio-caption">
                            <div class="portfolio-caption-heading">@item.Ad.ToUpper()</div>
                        </div>
                    </div>
                </div>
            }

